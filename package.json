{
  "name": "salemrealestate",
  "version": "1.0.0",
  "description": "Salem Real Estate Website - Full Stack Application",
  "scripts": {
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "cd client && npm run build",
    "start:prod": "cd server && npm start",
    "deploy:client": "cd client && node deploy.js",
    "deploy:server": "cd server && node deploy.js",
    "deploy": "npm run deploy:server && npm run deploy:client",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}